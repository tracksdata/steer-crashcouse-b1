package com;

public class Employee {

	int empId;
	String empName;
	double salary;

	void processSalary() {
		salary = salary + 10000;
	}

	public static void main(String[] args) {

		Employee e1 = new Employee();
		Employee e2 = new Employee();
		Employee e3 = new Employee();

		e1.empId = 10;
		e1.empName = "Sam";

		e2.empId = 11;
		e2.empName = "Bucky";

		e3.empId = 12;
		e3.empName = "Rose";

	}

}
