package com;
class C1{}
public class Sample {
	
	/*
	 *  1. fields -  behaviour
	 *  2. methods - actions
	 *  
	 * 		-> fields have common behaviour across all objects
	 * 		-> methods changes  behaviour of fields of a object
	 * 
	 *  		
	 * 
	 */
	
	// instance fields
	int x; // instance field
	static int y; // static field
	
	void increment() {
		x=x+1;
	}

	public static void main(String[] args) {
		
		
		int x=10;
		double y=76.45;
		String z="Abc";
		
		Sample s=new Sample();
		
		
		
	}
}
